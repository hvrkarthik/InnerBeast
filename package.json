{
  "name": "innerbeast",
  "version": "1.0.9",
  "private": true,
  "main": "expo-router/entry",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "EXPO_NO_TELEMETRY=1 npx expo start",
    "build:web": "expo export --platform web",
    "build:ios": "eas build --platform ios --non-interactive",
    "build:android": "eas build --platform android --non-interactive",
    "submit:ios": "eas submit --platform ios --non-interactive",
    "submit:android": "eas submit --platform android --non-interactive",
    "reset-project": "node ./scripts/reset-project.js",
    "lint": "expo lint"
  },
  "dependencies": {
    "expo": "53.0.22",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "@expo/vector-icons": "^14.1.0",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/space-grotesk": "^0.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "0.20.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-webview": "13.13.5",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.6.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "lucide-react-native": "^0.363.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "eslint": "^9.7.0",
    "eslint-config-expo": "^9.2.0"
  },
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}
